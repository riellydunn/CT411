function setup() {
    createCanvas(1000, 1000);
    background(255);
    noStroke();
};

function draw() {
    fill(249, 56, 56);
    ellipse(500, 350, 300, 300);
    fill(255);
    ellipse(465, 330, 30, 60);
    ellipse(535, 330, 30, 60);
    
    fill(50, 200, 74);
    ellipse(465, 343, 25, 32);
    ellipse(535, 343, 25, 32);
    
    fill(0);
    ellipse(465, 350, 14, 14);
    ellipse(535, 350, 14, 14);
    
    fill(255);
    arc(500, 385, 80, 80, TWO_PI, PI);
}